{
  "name": "timbuctoo-gui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "faker": "graphql-faker --extend http://localhost:8080/v5/graphql \\  -H 'Accept:application/json' -H 'Content-Type:application/json'",
    "prettify": "prettier --write './src/**/*.{tsx,ts}'",
    "precommit": "check-dependencies && lint-staged"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --write",
      "tslint --type-check --project .",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": true,
    "bracketSpacing": true
  },
  "dependencies": {
    "js-cookie": "^2.1.4",
    "lodash": "^4.17.4",
    "polished": "^1.7.0",
    "query-string": "^5.0.0",
    "react": "^16.0.0",
    "react-apollo": "^1.4.15",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-onclickoutside": "^6.5.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "^2.8.0",
    "react-sortable-hoc": "^0.6.7",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-connect": "^5.1.0",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "@types/async": "^2.0.31",
    "@types/express": "^4.0.37",
    "@types/graphql": "^0.11.0",
    "@types/jest": "^20.0.8",
    "@types/js-cookie": "^2.1.0",
    "@types/lodash": "^4.14.76",
    "@types/node": "^8.0.26",
    "@types/react": "^16.0.19",
    "@types/react-dom": "^16.0.0",
    "@types/react-helmet": "^5.0.3",
    "@types/react-onclickoutside": "^6.0.2",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.0.7",
    "@types/react-sortable-hoc": "^0.6.0",
    "@types/recompose": "^0.24.3",
    "@types/webpack-env": "^1.13.2",
    "check-dependencies": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "prettier": "1.8.2",
    "tslint-eslint-rules": "^4.1.1"
  }
}
