{
  "name": "timbuctoo-gui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "CI=true react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "faker": "graphql-faker --extend http://localhost:8080/v5/graphql \\  -H 'Accept:application/json' -H 'Content-Type:application/json'",
    "prettify": "prettier --write './src/**/*.{tsx,ts}'",
    "precommit": "check-dependencies && lint-staged"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --write",
      "tslint --project .",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": true,
    "bracketSpacing": true
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.2",
    "apollo-client": "^2.1.0",
    "apollo-link-context": "^1.0.2",
    "apollo-link-error": "^1.0.2",
    "apollo-link-http": "^1.3.0",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "js-cookie": "^2.1.4",
    "leaflet": "^1.3.4",
    "lodash": "^4.17.4",
    "markdown-it": "^8.4.2",
    "polished": "^1.9.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.0",
    "react": "^16.0.0",
    "react-apollo": "^2.0.1",
    "react-dom": "^16.0.0",
    "react-dropzone": "^4.2.3",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.7",
    "react-input-range": "^1.2.2",
    "react-leaflet": "^1.9.1",
    "react-onclickoutside": "^6.5.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-scripts-ts": "^2.16.0",
    "react-sortable-hoc": "^0.6.7",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-connect": "^5.1.0",
    "remarkable": "^1.7.1",
    "styled-components": "^2.1.2",
    "typescript": "^2.8.4",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.49",
    "@types/graphql": "^0.11.8",
    "@types/jest": "^20.0.8",
    "@types/js-cookie": "^2.1.0",
    "@types/lodash": "^4.14.116",
    "@types/markdown-it": "0.0.4",
    "@types/leaflet": "^1.2.9",
    "@types/node": "^8.10.23",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-dropzone": "^4.2.0",
    "@types/react-helmet": "^5.0.6",
    "@types/react-icons": "^2.2.5",
    "@types/react-leaflet": "^1.1.6",
    "@types/react-onclickoutside": "^6.0.3",
    "@types/react-redux": "^5.0.20",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-router-redux": "^5.0.15",
    "@types/react-sortable-hoc": "^0.6.3",
    "@types/recompose": "^0.26.3",
    "@types/uuid": "^3.4.3",
    "@types/webpack-env": "^1.13.6",
    "check-dependencies": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
